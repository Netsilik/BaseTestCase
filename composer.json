{
	"name": "netsilik/base-test-case",
	"description": "Base Test Case for PHPUnit, proving various extended options for unit testing.",
	"keywords": ["phpunit", "unit testing", "testing", "development"],
	"type": "library",
	"license": "MIT",
	"homepage": "https://gitlab.com/Netsilik/BaseTestCase",
	"authors": [
		{
			"name": "Jacco-V",
			"role": "Developer",
			"homepage": "http://netsilik.nl"
		}
	],
	"require": {
		"php": "^8.1",
		"phpunit/phpunit": "^8.3"
	},
	"require-dev": {
		"netsilik/d": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Netsilik\\Testing\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests",
			"Example\\App\\": "example/app",
			"Example\\Tests\\": "example/tests"
		}
	}
}
